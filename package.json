{
	"name": "nuxt-content-transformer-toml",
	"version": "0.2.0",
	"packageManager": "pnpm@10.26.1",
	"author": "Ray <i@mk1.io> (@so1ve)",
	"type": "module",
	"description": "A TOML transformer for Nuxt Content",
	"keywords": [
		"nuxt",
		"nuxt-content",
		"smol-toml",
		"toml",
		"transformer"
	],
	"homepage": "https://github.com/so1ve/nuxt-content-transformer-toml#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/so1ve/nuxt-content-transformer-toml.git"
	},
	"bugs": {
		"url": "https://github.com/so1ve/nuxt-content-transformer-toml/issues"
	},
	"license": "MIT",
	"sideEffects": false,
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown",
		"lint": "eslint . && prettier . --check",
		"lint:fix": "eslint . --fix && prettier . --write",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"test": "vitest",
		"typecheck": "tsgo --noEmit",
		"watch": "tsdown --watch"
	},
	"dependencies": {
		"smol-toml": "^1.4.2"
	},
	"devDependencies": {
		"@antfu/ni": "^27.0.1",
		"@nuxt/content": "^3.8.0",
		"@so1ve/eslint-config": "^3.11.2",
		"@so1ve/prettier-config": "^3.11.2",
		"@types/node": "^24.9.2",
		"@typescript/native-preview": "7.0.0-dev.20251222.1",
		"bumpp": "^10.3.1",
		"eslint": "^9.38.0",
		"prettier": "^3.6.2",
		"tsdown": "^0.15.11",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.5"
	},
	"peerDependencies": {
		"@nuxt/content": "^3"
	}
}
